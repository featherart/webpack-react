{
  "name": "basic-webpack",
  "version": "1.0.0",
  "description": "An experimentation repo for creating a prototype whose foundation is a react-based technology stack.",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "dev": "./node_modules/.bin/webpack-dev-server --progress --inline --hot --history-api-fallback --content-base ./tmp",
    "dev:test": "./node_modules/.bin/webpack-dev-server --inline --hot --config webpack.test.config.babel.js --port 8081 --open",
    "prod:webpack-compile-server": "DEBUG=webpack ./node_modules/.bin/webpack --progress --config webpack.server.config.babel.js ",
    "prod:webpack-compile-client": "NODE_ENV=production ./node_modules/.bin/webpack --progress --config webpack.config.babel.js ",
    "prod:run-server": "DEBUG=server node tmp/server.bundle.js",
    "prod": "npm run prod:webpack-compile-server && npm run prod:webpack-compile-client && npm run prod:run-server "
  },
